            <Array name="_" length="5" type="uint32_t" />
          </Array>
        </Array>
      </State>
      <OutputPin name="fire" messageTypeId="exchange">
        <OnSend><![CDATA[
		handler_log(0, "node_fire");
		deviceState->count++;
    deviceState->x[1][1][2]=2;
		  ]]></OnSend>
      </OutputPin>
      <ReadyToSend><![CDATA[
		handler_log(0, "node_rts");
			*readyToSend = 0; 
