    </MessageType>
  </MessageTypes>
  <DeviceTypes>
    <DeviceType id="node">
      <Properties>
        <Scalar name="max_count" type="uint32_t"/>
        <Array name = "a_C" length = "3">
          <Array name = "a_D" length = "4">
            <Tuple name = "t_D">
              <Scalar name = "a" type = "uint32_t"/>
