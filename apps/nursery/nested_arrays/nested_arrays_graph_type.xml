          <Array name = "_" length = "4">
            <Tuple name = "_">
              <Scalar name = "a" type = "uint32_t"/>
              <Scalar name = "b" type = "uint32_t"/>
              <Tuple name = "t_sD">
                <Scalar name = "x" type = "uint32_t"/>
